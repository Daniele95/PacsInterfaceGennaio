<Window x:Class="GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Title="PACS Interface" Height="450" Width="900">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="150*"/>
            <ColumnDefinition Width="900*"/>
        </Grid.ColumnDefinitions>
        <Frame  x:FieldModifier="public" x:Name="frame" Content="Frame" Grid.Column="1" HorizontalAlignment="Left" Height="420" VerticalAlignment="Top" Width="755"/>
        <StackPanel HorizontalAlignment="Left" Height="420" VerticalAlignment="Top" Width="113">
            <Button x:Name="QueryPage" Content="QueryPage" Click="QueryPageButton_Click"/>
            <Button x:Name="DownloadPage" Content="DownloadPage" Click="DownloadPageButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
